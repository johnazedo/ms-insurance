                },
                "name": {
                    "type": "string"
                }
            }
        },
        "phonelist.Model": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "phonelist.ModelResponse": {
            "type": "object",
            "properties": {
                "brand-code": {
                    "type": "string"
                },
                "brand-name": {
                    "type": "string"
                },
                "list-cellphone-model": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/phonelist.Model"
                    }
                }
            }
        },
        "simulation.BuyResponse": {
            "type": "object",
            "properties": {
                "cellphone-brand-code": {
                    "type": "string"
                },
                "cellphone-model-code": {
                    "type": "string"
                },
                "franchise": {
                    "type": "number"
                },
                "payment-id": {
                    "type": "string"
                },
                "user-id": {
                    "type": "string"
                },
                "value-per-month": {
                    "type": "number"
                }
            }
        },
        "simulation.Request": {
            "type": "object",
            "required": [
                "cellphone-brand-code",
                "cellphone-model-code",
                "user-id"
            ],
            "properties": {
                "cellphone-brand-code": {
                    "type": "string"
                },
                "cellphone-model-code": {
                    "type": "string"
                },
                "user-id": {
                    "type": "string"
                }
            }
        },
        "simulation.Response": {
            "type": "object",
            "properties": {
                "cellphone-brand-code": {
                    "type": "string"
                },
                "cellphone-model-code": {
                    "type": "string"
                },
                "franchise": {
                    "type": "number"
                },
                "user-id": {
                    "type": "string"
                },
                "value-per-month": {
                    "type": "number"
                }
            }
        }
    },
    "securityDefinitions": {
        "JWT": {
            "type": "apiKey",
            "name": "token",
            "in": "header"
        }
    }
